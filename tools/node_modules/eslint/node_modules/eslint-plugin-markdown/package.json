  "devDependencies": {
    "chai": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-eslint": "^5.0.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-release": "^1.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^2.2.5"
  },
  "engines": {
    "node": "^6.14.0 || ^8.10.0 || >=9.10.0"
  },
